{
  "name": "mapierhub",
  "version": "1.0.0",
  "description": "Multi-provider map data aggregation backend",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
