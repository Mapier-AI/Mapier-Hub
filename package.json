    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@hono/node-server": "^1.19.6",
    "@openrouter/ai-sdk-provider": "^1.2.8",
    "@supabase/supabase-js": "^2.84.0",
    "ai": "^5.0.104",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "hono": "^4.10.6",
    "ioredis": "^5.8.2",
    "openai": "^6.9.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
